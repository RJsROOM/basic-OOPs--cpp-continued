#include <iostream>

using namespace std;

class rectangle{
    public:
    int l,b;
    int area(){
        return l*b;
        
    }
    int perimeter(){
        return 2*(l+b);
    }
};

int main()
{
    
    rectangle r1;
    r1.l=10;
    r1.b=6;
    cout<<r1.area()<<" \n"<<r1.perimeter();

    return 0;
}
